{
  "compilerOptions": {
    "allowJs": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./built",
    "resolveJsonModule": true,
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "target": "ESNext"
  },
  "include": ["./src/**/*"]
}
