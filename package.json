{
  "name": "qotd-bot",
  "version": "1.0.0",
  "description": "Discord Bot for the leetcode_reddit question of the day.",
  "private": true,
  "engines": {
    "node": ">=16.6"
  },
  "scripts": {
    "build": "rimraf built && npm run lint && npm run prettier && tsc",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write .",
    "start": "node built/index.js"
  },
  "keywords": [
    "discord",
    "leetcode_reddit"
  ],
  "author": "Rafael Negron",
  "license": "ISC",
  "dependencies": {
    "discord.js": "14.14.1",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "node-cron": "3.0.3",
    "node-fetch": "3.3.2"
  },
  "devDependencies": {
    "@types/node-cron": "3.0.11",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "typescript": "5.3.3"
  }
}
